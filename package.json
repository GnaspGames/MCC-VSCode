{
    "name": "mcc",
    "displayName": "Minecraft Command Code",
    "description": "VS Code Colorizer language support for Minecraft Command Code (MCC) as used by Smelt (smelt.gnasp.com)",
    "version": "1.3.10",
    "publisher": "Nathan Liparski, Moesh, Gnasp",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mcc",
            "aliases": ["Minecraft Command Code", "mcc"],
            "extensions": [".mcc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mcc",
            "scopeName": "source.mcc",
            "path": "./syntaxes/mcc.tmLanguage"
        }]
    }
}